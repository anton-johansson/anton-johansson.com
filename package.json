{
  "name": "resume",
  "version": "0.1.0",
  "scripts": {
    "dev-client": "webpack --config ./webpack.client.js --mode development --watch",
    "dev-server": "webpack --config ./webpack.server.js --mode development --watch",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\" \"npm run start\"",
    "build-client": "webpack --config ./webpack.client.js --mode production",
    "build-server": "webpack --config ./webpack.server.js --mode production",
    "build": "npm run build-client && npm run build-server",
    "start": "cd build && nodemon --watch . ./bundle.js"
  },
  "dependencies": {
    "@babel/runtime": "7.7.4",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "express-rate-limit": "3.4.0",
    "got": "11.8.5",
    "html-minifier": "4.0.0",
    "isomorphic-style-loader": "5.1.0",
    "ky": "0.9.0",
    "nodemailer": "5.1.1",
    "react": "16.12.0",
    "react-anchor-link-smooth-scroll": "1.0.12",
    "react-dom": "16.12.0",
    "react-fittext": "1.0.0",
    "react-helmet-async": "1.0.4",
    "react-redux": "7.1.3",
    "react-waypoint": "9.0.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "concurrently": "5.3.0",
    "css-loader": "3.2.0",
    "file-loader": "4.3.0",
    "nodemon": "2.0.2",
    "react-hot-loader": "4.11.1",
    "style-loader": "1.0.0",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "1.7.2"
  }
}
