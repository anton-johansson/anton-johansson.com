{
  "name": "resume",
  "version": "0.1.0",
  "scripts": {
    "dev-client": "webpack --config ./webpack.client.js --mode development --watch",
    "dev-server": "webpack --config ./webpack.server.js --mode development --watch",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\" \"npm run start\"",
    "build-client": "webpack --config ./webpack.client.js --mode production",
    "build-server": "webpack --config ./webpack.server.js --mode production",
    "build": "npm run build-client && npm run build-server",
    "start": "nodemon --watch build build/bundle.js"
  },
  "dependencies": {
    "@babel/runtime": "7.7.4",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "express-rate-limit": "3.4.0",
    "got": "9.6.0",
    "ky": "0.9.0",
    "nodemailer": "5.1.1",
    "react": "16.6.0",
    "react-anchor-link-smooth-scroll": "1.0.12",
    "react-dom": "16.6.0",
    "react-fittext": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "file-loader": "4.3.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "react-hot-loader": "4.11.1",
    "style-loader": "1.0.0",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
